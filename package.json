    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.18.1",
    "bootstrap": "^4.5.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "infinite-react-carousel": "^1.2.11",
    "mongoose": "^5.3.16",
    "node-sass": "^4.14.1",
