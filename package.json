    "@material-ui/icons": "^4.9.1",
    "axios": "^0.18.1",
    "bootstrap": "^4.5.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "firebase": "^7.22.1",
    "if-env": "^1.0.4",
    "material-ui-banner": "^1.4.3",
    "material-ui-search-bar": "^1.0.0",
    "mongoose": "^5.3.16",
    "node-sass": "^4.14.1",
    "particlesjs": "^2.2.3",
